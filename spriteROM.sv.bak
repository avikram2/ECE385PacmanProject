module  pacmanROM
    (
            input [18:0] read_address,
            input Clk,
    
            output logic [23:0] q
    );
    
    // mem has width of 24 bits and a total of 400 addresses
    logic [23:0] mem [0:63];
    
    initial
    begin
         $readmemh("pacman.txt", mem);
    end
    
    
    always_ff @ (posedge Clk) begin
        q <= mem[read_address]; 
    end
    
    endmodule
    